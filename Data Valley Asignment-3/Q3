//method Overloading
class Calculator {
  public int add(int a, int b) {
    return a + b;
  }

  public double add(double a, double b) {
    return a + b;
  }

  public int add(int a, int b, int c) {
    return a + b + c;
  }
}


// method Overriding
class Vehicle {
  public void move() {
    System.out.println("Vehicle is moving");
  }
}

class Bike extends Vehicle {
  @Override
  public void move() {
    System.out.println("Bike is moving safely");
  }
}

